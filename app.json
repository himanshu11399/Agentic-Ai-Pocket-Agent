{
  "expo": {
    "name": "Pocket Agent",
    "slug": "PocketAgent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ai-roboimag.jpg",
    "scheme": "frontened",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.himanshu1485.Frontened"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/mainicon.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.himanshu1485.Frontened"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/mainicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mainicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9a12cec5-6976-4222-9242-c2250c9a4894"
      }
    }
  }
}
